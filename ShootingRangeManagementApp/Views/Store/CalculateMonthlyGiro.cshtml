            <div class="row">
                <form action="CalculateMonthlyGiro" method="post">
                    <div class="form-group">
                        <input asp-for="StoreId" type="hidden" value="@ViewData["Id"]" />
                        <div class="col-lg-3 col-6 offset-4">
                            Start Date:<input type="date" class="form-control" name="startDate" />
                        </div>
                    </div>
                    <br />
                    <div class="form-group">
                        <div class="col-lg-3 col-6 offset-4">
                            End Date:<input type="date" class="form-control" name="endDate" />
                        </div>
                    </div>

                    <div class="col-lg-3 col-6 offset-4">
                        <input type="submit" value="Submit" class="btn btn-outline-secondary" /><br />
                        <br />
                        <a class="text-muted" asp-page="/CalculateMonthlyGiro">Calculate with filter</a>
                    </div>
                </form>

            </div>
            <br />




            <table class="mt-4 table table-hover table-bordered table-sm">
                <thead>
                    <tr>
                        <td>Store Partners</td>
                        <td>Payment Rate</td>
                        <td>Total Amount</td>

                    </tr>
                </thead>

                <tbody>

                    @foreach (var storePartner in Model.StorePartners)
                    {

        <tr>
            <td>

                @storePartner.Name

            </td>
            <td>@storePartner.PaymentRate</td>
            <td>
                @storePartner.TotalAmount

            </td>
        </tr>
}




                </tbody>
            </table>
        </div>

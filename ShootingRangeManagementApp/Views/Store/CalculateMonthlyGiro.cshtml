@model MonthlyGiroViewModel
    <section class="content">
        <div class="container-fluid">
            @*<h3 class="text-md-center">This is calculating monthly giro page.</h3>*@
            <h4 class="text-md-center">
                <i class="fas fa-globe mt-4 "></i> @Model.Store.StoreName

            </h4>
            <br />
            <div class="row">
                <div class="col-lg-3 col-6 ml-5">
                    <!-- small card -->
                    <div class="small-box bg-info">
                        <div class="inner">
                            <h3>@Model.totalIncome</h3>

                            <p>Total Income this month:</p>
                        </div>
                        <div class="icon">
                            <i class="fas fa-shopping-cart"></i>
                        </div>
                        <a href="#" class="small-box-footer">
                            More info <i class="fas fa-arrow-circle-right"></i>
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-6 ml-5">
                    <!-- small card -->
                    <div class="small-box bg-danger">
                        <div class="inner">
                            <h3>@Model.totalExpense</h3>

                            <p>Total Expense</p>
                        </div>
                        <div class="icon">
                            <i class="fas fa-chart-pie"></i>
                        </div>
                        <a href="#" class="small-box-footer">
                            More info <i class="fas fa-arrow-circle-right"></i>
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-6 ml-5">
                    <!-- small card -->
                    <div class="small-box bg-success">
                        <div class="inner">
                            <h3>@Model.Total</h3>

                            <p>Total</p>
                        </div>
                        <div class="icon">
                            <i class="ion ion-stats-bars"></i>
                        </div>
                        <a href="#" class="small-box-footer">
                            More info <i class="fas fa-arrow-circle-right"></i>
                        </a>
                    </div>
                </div>
            </div>
